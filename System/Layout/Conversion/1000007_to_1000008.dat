#include "1.00.0008.dat"

// *************************************************************
//  Upgrade Stub
// *************************************************************
bool upgrade( World @ world, uint sourceVersion, uint destinationVersion )
{
    // Check supported versions
    if ( sourceVersion != 0x1000007 || destinationVersion != 0x1000008 ) // v1.00.0007 -> v1.00.0008
        return false;

    // Upgrade to v1.00.0008
    return upgrade_1000007_to_1000008( world );
}