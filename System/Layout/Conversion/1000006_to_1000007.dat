#include "1.00.0007.dat"

// *************************************************************
//  Upgrade Stub
// *************************************************************
bool upgrade( World @ world, uint sourceVersion, uint destinationVersion )
{
    // Check supported versions
    if ( sourceVersion != 0x1000006 || destinationVersion != 0x1000007 ) // v1.00.0006 -> v1.00.0007
        return false;

    // Upgrade to v1.00.0007
    return upgrade_1000006_to_1000007( world );
}